<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <layout class="ch.qos.logback.classic.PatternLayout">
            <Pattern>[%level] %msg%n</Pattern>
        </layout>
    </appender>
    <root>
        <level value="ALL" />
    </root>
</configuration>
